<%--Bootstrap Alerts--%>
    <div id="localAlertDangerBlock" class="alert alert-danger" role="alert" style="display: none">
        <a class="close" data-dismiss="alert"><span class="glyphicon glyphicon-remove"></span></a>
        <%--<a class="close" onclick="$('.alert').hide()"><span class="glyphicon glyphicon-remove"></span></a>--%>
        <div class="localMessageDanger">TEXT</div>
    </div>
    <div id="localAlertWarningBlock" class="alert alert-warning" role="alert" style="display: none">
        <a class="close" data-dismiss="alert"><span class="glyphicon glyphicon-remove"></span></a>
        <div class="localMessageWarning">TEXT</div>
    </div>
    <div id="localAlertInfoBlock" class="alert alert-info" role="alert" style="display: none">
        <a class="close" data-dismiss="alert"><span class="glyphicon glyphicon-remove"></span></a>
        <div class="localMessageWarning">TEXT</div>
    </div>
    <div id="localAlertSuccessBlock" class="alert alert-success" role="alert" style="display: none">
        <a class="close" data-dismiss="alert"><span class="glyphicon glyphicon-remove"></span></a>
        <div class="localMessageWarning">TEXT</div>
    </div>

